{
  "name": "testbook",
  "version": "1.0.0",
  "description": "Express TypeScript Backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:concurrent": "ts-node scripts/test-concurrent.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-validator": "^7.3.1",
    "mongoose": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.19.27",
    "mongodb-memory-server": "^11.0.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.16"
  }
}
